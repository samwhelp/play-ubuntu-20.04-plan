
default: help
.PHONY: default

help:
	@echo 'Usage:'
	@echo '	$$ make [command]'
	@echo
	@echo 'Ex:'
	@echo '	$$ make'
	@echo '	$$ make help'
	@echo
	@echo '	$$ make to_example'
	@echo '	$$ make to_example_dark'
	@echo



.PHONY: help


to_example:
	@./default-change.sh example
.PHONY: example

to_example_dark:
	@./default-change.sh example-dark
.PHONY: to_example_dark


